{
  "name": "weex-demo-project",
  "description": "A weex project with dolphin weex ui",
  "version": "0.0.4",
  "main": "",
  "keywords": [
    "weex",
    "vue"
  ],
  "author": "",
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run clean:all && webpack --watch --mode=development",
    "build": "npm run clean:all && webpack --mode=production",
    "build:zip": "npm run clean:all && webpack --mode=production && node ./build/zip.js",
    "clean:all": "rimraf ./entry && rimraf ./dist && rimraf ./release",
    "lint": "eslint --ext .js,.vue src",
    "lint:fix": "eslint --ext .js,.vue src --fix",
    "prepare": "husky install",
    "cz": "cz",
    "release": "standard-version",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "prettier": "prettier --write \"src/\"",
    "adb": "node ./build/adb"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "dependencies": {
    "dayjs": "^1.11.2",
    "dolphin-native-bridge": "^0.1.20",
    "dolphin-weex-ui": "^0.3.3",
    "lodash": "^4.17.21",
    "vue": "^2.6.14",
    "vuex": "3.6.2",
    "weex-bindingx": "0.0.49",
    "weex-vue-render": "^1.0.36"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "clean-webpack-plugin": "^4.0.0",
    "commitizen": "^4.2.4",
    "compressing": "^1.5.1",
    "css-loader": "^6.7.1",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.3.0",
    "dotenv": "^16.0.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^5.2.3",
    "file-loader": "^6.2.0",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "husky": "^8.0.1",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "prettier": "^2.6.2",
    "rimraf": "^2.6.2",
    "sass": "^1.52.3",
    "sass-loader": "^13.0.0",
    "standard-version": "^9.5.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "vue-style-loader": "^3.0.3",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-merge": "^5.8.0",
    "weex-loader": "^0.7.12"
  }
}
